{
    "name": "fractal-thinking-mcp-server",
    "version": "1.0.0",
    "description": "MCP server for fractal thinking analysis",
    "main": "build/fractal-thinking-mcp.js",
    "type": "module",
    "bin": {
        "fractal-thinking-mcp-server": "./build/fractal-thinking-mcp.js"
    },
    "scripts": {
        "build": "tsc",
        "start": "node build/fractal-thinking-mcp.js",
        "dev": "node --loader ts-node/esm src/fractal-thinking-mcp.ts",
        "prepare": "npm run build"
    },
    "keywords": [
        "mcp",
        "fractal",
        "thinking",
        "ai",
        "llm"
    ],
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.8.0",
        "@types/uuid": "^10.0.0",
        "chalk": "^4.1.2",
        "uuid": "^11.1.0",
        "zod": "^3.22.0"
    },
    "devDependencies": {
        "@types/node": "^20.4.5",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    }
}
